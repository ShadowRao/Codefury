{
	"manifest_version": 2,
	"name": "Codefury 6.0",
	"description": "Fetches current tab url and tracks the time.",
	"version": "0.1",
	"author": "Shadow",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Just observing your current url."
	},
	"permissions": [
		"tabs",
		"activeTab",
		"http://127.0.0.1:5000/*",
		"storage"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	}
}